{
	"name": "jrfnl/composer-script-poc",
	"description": "Just testing some things.",
	"require": {
		"php": "^7.0"
	},
	"autoload-dev": {
		"classmap": [
			"Composer/"
		]
	},
	"scripts": {
		"return-zero": [
			"Jrfnl\\Composer\\Actions::return_zero"
		],
		"return-one": [
			"Jrfnl\\Composer\\Actions::return_one"
		],
		"return-more": [
			"Jrfnl\\Composer\\Actions::return_more"
		],
		"exit-zero": [
			"Jrfnl\\Composer\\Actions::exit_zero"
		],
		"exit-one": [
			"Jrfnl\\Composer\\Actions::exit_one"
		],
		"exit-two-plus": [
			"@putenv YOASTCS_THRESHOLD_ERRORS=255",
			"@putenv YOASTCS_THRESHOLD_WARNINGS=222",
			"Jrfnl\\Composer\\Actions::exit_two"
		],
		"exit-more": [
			"Jrfnl\\Composer\\Actions::exit_more"
		],
		"exit-var": [
			"Jrfnl\\Composer\\Actions::exit_variable"
		],
		"exit-two-no-const": [
			"Jrfnl\\Composer\\Actions::exit_two_no_const"
		],
		"exit-two-with-const": [
			"Jrfnl\\Composer\\Actions::exit_two_with_const"
		],
		"exit-two-set-const": [
			"Jrfnl\\Composer\\Actions::exit_two_set_const"
		],
		"exit-more-no-const": [
			"Jrfnl\\Composer\\Actions::exit_more_no_const"
		],
		"exit-two-no-env": [
			"Jrfnl\\Composer\\Actions::exit_two_no_env"
		],
		"exit-two-with-env": [
			"Jrfnl\\Composer\\Actions::exit_two_with_env"
		],
		"exit-two-set-env": [
			"Jrfnl\\Composer\\Actions::exit_two_set_env"
		],
		"exit-more-no-env": [
			"Jrfnl\\Composer\\Actions::exit_more_no_env"
		]
	}
}
